{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext" ,
    "moduleResolution": "node" ,
    "lib": [
      "DOM",
      "ESNext"
    ] ,
    "jsx": "react" ,
    "noEmit": true ,
    "esModuleInterop": true ,
    "strict": true ,
    "skipLibCheck": true ,
    "forceConsistentCasingInFileNames": true ,
    "resolveJsonModule": true,
    "allowJs": true ,
    "checkJs": true ,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}